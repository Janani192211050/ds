#include<stdio.h>
   int linear(int [],int );
   int main()
   {
   int keyEle,i;
   int arr[10];
   printf("Enter 10 elements:");
   for(i=0;i<5;i++)
   {
   scanf("%d",&arr[i]);
   }
   printf("Enter the element to search: ");
   scanf("%d", &keyEle);
   if(linear(arr,keyEle))
   printf("The element is found\n");
   else
   printf("The element is not found\n");
   }
   int linear(int arr[],int keyEle)
   {
   for(int i=0;i<7;i++)
   {
   if(arr[i]==keyEle){
   return 1;
   }
   }
   return 0;
}
